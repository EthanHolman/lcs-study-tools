[{"spanishExample":"El *son* de Maria *will be here* para *then.*","englishTranslation":"Maria’s son will be here by then.","spanglish":"spanglish","vocabIncluded":["\"de\": \"of\", indicating possession","\"el\": \"the\" for masculine nouns","\"para\": \"by\" a moment in time, as in \"by then\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2070,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:06:22Z","errorCoachReport":false},{"spanishExample":"Eso no *will be* para *him.*","englishTranslation":"That won’t be for him.","spanglish":"spanglish","vocabIncluded":["\"eso\": \"that\", neuter (without a gendered noun)","\"no\": \"not\", or \"n't\" in \"didn't\", \"wouldn't\", etc.","\"para\": \"for\", as in \"intended for\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2071,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:06:33Z","errorCoachReport":false},{"spanishExample":"Eso *was created* por un *interesting guy.*","englishTranslation":"That was created by an interesting guy.","spanglish":"spanglish","vocabIncluded":["\"eso\": \"that\", neuter (without a gendered noun)","\"un\": \"a\" or \"an\" with masculine nouns","por: \"by\", as in \"produced by\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2072,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:06:50Z","errorCoachReport":false},{"spanishExample":"*He* no me *brought* por *this street.*","englishTranslation":"He didn’t bring me along this street.","spanglish":"spanglish","vocabIncluded":["\"me\": \"me\" as a direct object","\"no\": \"not\", or \"n't\" in \"didn't\", \"wouldn't\", etc.","por: \"along\", as in \"along the route\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2073,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:07:05Z","errorCoachReport":false},{"spanishExample":"*I have to leave* a 2:00 y *he* no *can.*","englishTranslation":"I have to leave at 2:00 and he can’t.","spanglish":"spanglish","vocabIncluded":["\"a\": \"at\" indicating a time","\"no\": \"not\", or \"n't\" in \"didn't\", \"wouldn't\", etc.","\"y\": \"and\" between clauses"],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2074,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:07:21Z","errorCoachReport":false},{"spanishExample":"*I* la *saw* en *the store* con *her mom.*","englishTranslation":"I saw her in the store with her mom.","spanglish":"spanglish","vocabIncluded":["\"en\": \"in\"","\"la\": \"her\" or \"it\" as a direct object","con: \"with\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2075,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:07:35Z","errorCoachReport":false},{"spanishExample":"*I said* que *he was* por *here.*","englishTranslation":"I said he was around here.","spanglish":"spanglish","vocabIncluded":["\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","por: \"near\"/\"around\", as in \"near here\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2076,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:07:48Z","errorCoachReport":false},{"spanishExample":"*I said* que *my coat* de *fur is* de *Europe.*","englishTranslation":"I said that my fur coat is from Europe.","spanglish":"spanglish","vocabIncluded":["\"de\" to mean \"of\" or \"from\", especially in terms of origin","\"de\": \"of\", indicating material/components","\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)"],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2077,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-09-08T16:19:06Z","errorCoachReport":false},{"spanishExample":"*I went* a *the store* por *the afternoon.*","englishTranslation":"I went to the store during the afternoon.","spanglish":"spanglish","vocabIncluded":["\"a\": \"to\", indicating direction/movement","por: \"for\"/\"during\", as in \"for an hour\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2078,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:08:10Z","errorCoachReport":false},{"spanishExample":"*It’s* una *girl* que *I know,* por eso *she* te *knows.*","englishTranslation":"It’s a girl that I know; that’s why she knows you.","spanglish":"spanglish","vocabIncluded":["\"por eso\": \"that's why\" or \"because of that\"","\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","\"te\": \"you\" as a direct object","\"una\": \"a\" or \"an\" with feminine nouns"],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2079,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:08:26Z","errorCoachReport":false},{"spanishExample":"La *actress is* en *the stage.*","englishTranslation":"The actress is on the stage.","spanglish":"spanglish","vocabIncluded":["\"en\": \"on\"","\"la\": \"the\" for feminine nouns"],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2080,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:08:35Z","errorCoachReport":false},{"spanishExample":"*She ran out* de *there* por *the fire.*","englishTranslation":"She ran out of there because of the fire.","spanglish":"spanglish","vocabIncluded":["\"de\": \"from\", indicating direction/movement","por: \"because of\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2081,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:08:47Z","errorCoachReport":false},{"spanishExample":"*She* y *I* lo *saw* en *the car.*","englishTranslation":"She and I saw him at the car.","spanglish":"spanglish","vocabIncluded":["\"en\": \"at\" indicating location","\"lo\": \"him\" or \"it\" as a direct object","\"y\": \"and\" between items in a list"],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2082,"dateCreated":"2023-04-25T20:35:17Z","dateModified":"2024-10-30T19:09:00Z","errorCoachReport":false}]