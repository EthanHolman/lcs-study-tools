[{"spanishExample":"¡Qué *embarrassing!*","englishTranslation":"How embarrassing!","spanglish":"spanglish","vocabIncluded":["qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2137,"dateCreated":"2023-05-02T13:10:33Z","dateModified":"2024-10-25T12:52:35Z","errorCoachReport":false},{"spanishExample":"¡Qué *strange* que *this is translated into Spanglish!*","englishTranslation":"¡How strange that this is translated into Spanglish!","spanglish":"spanglish","vocabIncluded":["\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2138,"dateCreated":"2023-05-02T13:10:33Z","dateModified":"2024-10-25T12:53:01Z","errorCoachReport":false},{"spanishExample":"¿No *you can do better* que eso?","englishTranslation":"You can’t do better than that?","spanglish":"spanglish","vocabIncluded":["\"eso\": \"that\", neuter (without a gendered noun)","\"no\": \"not\", or \"n't\" in \"didn't\", \"wouldn't\", etc.","\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)"],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2139,"dateCreated":"2023-05-02T13:10:33Z","dateModified":"2024-10-25T12:53:10Z","errorCoachReport":false},{"spanishExample":"¿Por qué *did you walk* por *this road* en *the snow?*","englishTranslation":"Why did you walk along this road in the snow?","spanglish":"spanglish","vocabIncluded":["\"en\": \"in\"","por qué: \"why?\" (idiom)","por: \"along\", as in \"along the route\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2140,"dateCreated":"2023-05-02T13:10:33Z","dateModified":"2024-10-25T12:53:30Z","errorCoachReport":false},{"spanishExample":"¿Qué es eso?","englishTranslation":"What is that?","spanglish":"esp","vocabIncluded":[