[{"spanishExample":"¡Siga sus ideas!","englishTranslation":"(Formal) Follow your ideas!","spanglish":"esp","vocabIncluded":[],"vocabComplete":false,"spanishAudioLa":"","englishAudio":"","recordId":11935,"dateCreated":"2023-11-02T18:43:25Z","dateModified":"2023-11-02T18:43:25Z","errorCoachReport":false},{"spanishExample":"¡Sigan los pasos que les di!","englishTranslation":"(Plural) Follow the steps I gave you!","spanglish":"esp","vocabIncluded":[],"vocabComplete":false,"spanishAudioLa":"","englishAudio":"","recordId":11937,"dateCreated":"2023-11-02T18:43:25Z","dateModified":"2023-11-02T18:43:25Z","errorCoachReport":false},{"spanishExample":"¡Sígueme a la escuela!","englishTranslation":"Follow me to the school!","spanglish":"esp","vocabIncluded":[],"vocabComplete":false,"spanishAudioLa":"","englishAudio":"","recordId":11941,"dateCreated":"2023-11-02T18:43:25Z","dateModified":"2023-11-02T18:43:25Z","errorCoachReport":false},{"spanishExample":"Acaba de seguir la línea.","englishTranslation":"She just now followed the line.","spanglish":"esp","vocabIncluded":[],"vocabComplete":false,"spanishAudioLa":"","englishAudio":"","recordId":11944,"dateCreated":"2023-11-02T18:43:25Z","dateModified":"2023-11-02T18:43:25Z","errorCoachReport":false},{"spanishExample":"Ellos siguen trabajando, pero yo no sigo.","englishTranslation":"They continue working, but I’m not continuing.","spanglish":"esp","vocabIncluded":[],"vocabComplete":false,"spanishAudioLa":"","englishAudio":"","recordId":11950,"dateCreated":"2023-11-02T18:43:25Z","dateModified":"2023-11-02T18:43:25Z","errorCoachReport":false},{"spanishExample":"Me ha estado siguiendo.","englishTranslation":"She has been following me.","spanglish":"esp","vocabIncluded":[],"vocabComplete":false,"spanishAudioLa":"","englishAudio":"","recordId":11957,"dateCreated":"2023-11-02T18:43:25Z","dateModified":"2023-11-02T18:43:25Z","errorCoachReport":false},{"spanishExample":"Mi amigo ha seguido mi trabajo.","englishTranslation":"My friend has follo