[{"spanishExample":"¡Qué *cool* que *he is*con *her!*","englishTranslation":"How cool that he’s with her!","spanglish":"spanglish","vocabIncluded":["\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","con: \"with\"","qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2114,"dateCreated":"2023-05-02T12:43:52Z","dateModified":"2024-10-30T18:59:25Z","errorCoachReport":false},{"spanishExample":"¡Qué *funny* que eso no *was*para *him!*","englishTranslation":"How funny that that wasn’t for him!","spanglish":"spanglish","vocabIncluded":["\"eso\": \"that\", neuter (without a gendered noun)","\"no\": \"not\", or \"n't\" in \"didn't\", \"wouldn't\", etc.","\"para\": \"for\", as in \"intended for\"","\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2115,"dateCreated":"2023-05-02T12:43:52Z","dateModified":"2024-10-30T18:59:37Z","errorCoachReport":false},{"spanishExample":"¡Qué *nice* que la *girl* la *found!*","englishTranslation":"How nice that the girl found her!","spanglish":"spanglish","vocabIncluded":["\"la\": \"her\" or \"it\" as a direct object","\"la\": \"the\" for feminine nouns","\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2116,"dateCreated":"2023-05-02T12:43:52Z","dateModified":"2024-10-30T18:59:51Z","errorCoachReport":false},{"spanishExample":"¡Qué *sad* que la *girl* no *won!*","englishTranslation":"How sad that the girl didn’t win!","spanglish":"spanglish","vocabIncluded":["\"la\": \"the\" for feminine nouns","\"no\": \"not\", or \"n't\" in \"didn't\", \"wouldn't\", etc.","\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2117,"dateCreated":"2023-05-02T12:43:52Z","dateModified":"2024-10-30T19:00:08Z","errorCoachReport":false},{"spanishExample":"¡Qué *scary* que *you almost crashed!*","englishTranslation":"How scary that you almost crashed!","spanglish":"spanglish","vocabIncluded":["\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2118,"dateCreated":"2023-05-02T12:43:52Z","dateModified":"2024-10-30T19:00:19Z","errorCoachReport":false},{"spanishExample":"¡Qué *strange* que *he’s here!*","englishTranslation":"How strange that he’s here!","spanglish":"spanglish","vocabIncluded":["\"que\", meaning \"that\" as a conjunction (even in cases where \"that\" disappears in English)","qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2119,"dateCreated":"2023-05-02T12:43:52Z","dateModified":"2024-10-30T19:00:30Z","errorCoachReport":false},{"spanishExample":"¡Qué *terrible!*","englishTranslation":"How terrible!","spanglish":"spanglish","vocabIncluded":["qué: \"what!\" or \"how!\" in exclamations, as in \"how lucky!\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2120,"dateCreated":"2023-05-02T12:43:52Z","dateModified":"2024-10-30T19:00:40Z","errorCoachReport":false},{"spanishExample":"¿Por qué *did he go* por *this street?*","englishTranslation":"Why did he go along this street?","spanglish":"spanglish","vocabIncluded":["por qué: \"why?\" (idiom)","por: \"along\", as in \"along the route\""],"vocabComplete":true,"spanishAudioLa":"","englishAudio":"","recordId":2121,"